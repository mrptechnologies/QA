 /**
  * To disable the attributes of Validation
  */
 function freezeTab(tabName:String) {

	 if(employerApplicationStatus=="E_SUBMITTED" || employerApplicationStatus=="PAPER_SUBMITTED" || employerApplicationStatus=="SENT_TO_UNDERWRITING"){
		
		if(tabName=="GroupCompanyInformation"){
			disableGroupCompanyInformation(false);
		}else if(tabName=="EnrollmentCriteria"){
			disableEnrollmentCriteria(false);
		}else if(tabName=="MMO"){
			disableMMO(false);
		}else if(tabName=="DentalPlans"){
			disableDentalPlans(false);
		}else if(tabName=="VisionServicePlans"){
			disableVisionServicePlans(false);	
		}else if(tabName=="FortDearbornLICPlans"){
			disableFortDearbornLICPlans(false);
		}else if(tabName=="CarrierHistory"){
			disableCarrierHistory(false);
		}else if(tabName=="Validation"){
			disableValidation(false);
		}else if(tabName=="AuthorizedSignature"){
			disableAuthorizedSignature(false);
			Submit_AuthorizedSign._visible = false;
			Submit_Complete._visible = true;
		
			_root.mMsg1._visible = false;
			_root.mMsg2._visible = false;
			_root.mMsg3a._visible = true;
			_root.mMsg3b._visible = true;
		}
		
	 }else if(tabName=="AuthorizedSignature"){
		 _root.mMsg1._visible = true;
		_root.mMsg2._visible = false;
		_root.mMsg3a._visible = false;
		_root.mMsg3b._visible = false;
		Submit_AuthorizedSign._visible = true;
		Submit_Complete._visible = false;
	 }
 }
 
